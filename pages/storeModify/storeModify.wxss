.headscing {
  height: 88rpx;
  line-height: 88rpx;
  font-size: 30rpx;
  padding: 0 0 0 20rpx;
}
.scingIcon {
  width: 22rpx;
  height: 28rpx;
  margin: 0 14rpx 0 0;
  position: relative;
  top: 2rpx;
 }
 .inputtings {
  display: flex;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 28rpx;
  padding: 0 0 0 20rpx;
  border-bottom: 1rpx solid #e4e4e4;
  color: #666666;
  justify-content: space-between;
}
.topics {
  margin: 0 40rpx 0 0;
}
.inputtings input {
  height: 88rpx;
  text-align: right;
  padding-right: 20rpx;
  color: #333;
}

.upPhotoBox {
  float: left;
}
.upPhotoBox image {
  width: 100%;
  height: 100%;
}
.upPhotoBox .addPdown {
  width: 150rpx;
  height: 150rpx;
  line-height: 150rpx;
  border: 2rpx solid #ccc;
  text-align: center;
  color: #ccc;
  font-size: 70rpx;
  float: left;
  margin: 10rpx 10rpx 0 0;
}
.upImage {
  padding: 27rpx 0 27rpx 20rpx;
  overflow: hidden;
  display: flex;
}
.upImage .topics {
  color: #666666;
  flex: 0 0 132rpx;
  font-size: 28rpx;
  float: left;
}
.upNum {
  font-size: 28rpx;
  color: #666666;
  padding: 0 20rpx 0 0;
}
.videoBox {
  width: 100%;
  height: 450rpx;
  margin: 0 auto;
}
.videoBtn {
  border: 1rpx solid #e4e4e4;
  font-size: 28rpx;
  padding: 16rpx 30rpx;
}

.store {
  color: #fff;
  font-size: 34rpx;
  background-color: #FF6B4E;
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  position: fixed; 
  bottom: 0;
  left: 0;
  z-index: 999;
}