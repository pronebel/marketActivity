#searchBar{
  background: #FAFAFA;
  padding:20rpx;
  display: block;
  position: fixed;
  box-sizing: border-box;
  width: 100%;
  top: 0;
  z-index: 99
}
#searchBar input{
  border:1px solid #E4E4E4;
  border-radius: 6rpx;
  background: #fff;
  text-align: center;
  height: 56rpx;

}
.icon-search{
  position: absolute;
  top: 38rpx;
  z-index: 9;
  left: 42%;
  font-size: 26rpx
}
.helplist{
  margin: 100rpx auto;
  padding:20rpx;
}
.helplist li{
  width:100%;
  background:#fff;
  height:236rpx;
  border-radius:12rpx;
  margin-bottom:20rpx;
  padding:20rpx;
  box-sizing:border-box;
  border: 1px solid #E4E4E4;
  line-height:50rpx
}
.list_con{
  align-content:space-around;
  
}
.helplist image{
  width:350rpx;
  height:194rpx;
  float:left;
  margin-right:20rpx;
}
.helplist p{
  margin:10rpx 0 10rpx;
  font-size: 32rpx
}
.red{
  color: #FF6B4E;
  font-size: 24rpx
}
.helplist button{
  display: block;
  widows: 250rpx;
  height:65rpx; 
  background: #FF6B4E ;
  border-radius:100rpx;
  color: #fff;
  line-height: 60rpx;
  font-size: 26rpx;
  margin-top: 15rpx
}

.showMe{
  display: inline-block
}
.hideMe{
  display: none
}
.container {
align-items:center;
justify-content:space-between;
box-sizing:border-box;
font-size:28rpx;
color:#000;
font-family:"微软雅黑";
}
