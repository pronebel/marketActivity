page{
  padding-top: 40rpx
}
.title{
  text-align: center;
  color: #999;
  margin-bottom: 30rpx
}
.card{
  padding: 0 20rpx;
  position: relative;
  height: 410rpx
}
.redbar{
  width: 100%;
  height: 16rpx;
  background: #FF6B4E;
  border-radius: 100rpx 
}
.mainbody{
  width:91%;
  box-sizing: border-box;
  background: #fff;
  position: absolute;
  top: 10rpx;
  left: 35rpx;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
  box-shadow:0 2px 7px 0 rgba(0,0,0,0.1);
  padding: 40rpx 20rpx
}
.mainbody li{
  line-height: 88rpx;
  border-bottom: 1px solid #E4E4E4 
}
 .mainbody li:last-child{
  border-bottom:none
} 
.mainbody text{
  width: 90rpx;
  display: inline-block;
  text-align-last: justify
}
.mainbody span{
  display: inline-block;
  margin-left: 10rpx
}

radio{
  color: #FF6B4E;
  margin-left: 20rpx
}
.mainbody em{
  color: #999;
  font-size: 22rpx;
  margin-left: 10rpx
}
button{
  background:#FF6B4E;
  color:#fff;
  border-radius:100rpx;

}
.bt{
   padding: 0 30rpx;
}
.tick{text-align:center;margin-bottom:20rpx}
 input{
  height:20rpx;
  line-height: 20rpx;
  margin-top: 18rpx;
  width: 500rpx
} 