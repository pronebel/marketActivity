/* 模态窗口 */

.mask {
  width:100%;
  height:100%;
  position:fixed;
  top:0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index:999; 
  box-sizing: border-box;
}
.maskBgPop{
  width:100%;
  height:100%;
  background:rgba(0,0,0,0.8);

}
.maskPopContent{
  position: absolute;
  bottom: 0;
  left: 0;
}
/*没有信息的提示背景图  */
.null_product{

    position: fixed;
    left: 0;
    /* top: 120rpx; */
    top: 0;
    bottom: 0;
    right: 0;
    /* background: #fff; */
    padding-top: 260rpx;
    background-color: #eee;
}
.null_product span {
  display:block;
  background:url("http://image.itiewan.com/no-order-l1.png") no-repeat;
  background-size:230rpx;
  width:230rpx;
  height:230rpx;
  margin:80rpx auto 30rpx auto; 
}
.null_product p{
    display: block;
    font-size: 30rpx;
    color: #999;
    line-height: 60rpx;
    text-align: center;
}
